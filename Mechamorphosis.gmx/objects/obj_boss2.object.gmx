            numAttacks = 0;
            combatState = "stand";
            audio_play_sound(snd_enemyDeath,5,false);
        }
    }
    else
    {
        if (soundTimer &gt;= 20)
        {
            audio_play_sound(snd_clank,5,false);
            soundTimer = 0;
        }
    }
}
