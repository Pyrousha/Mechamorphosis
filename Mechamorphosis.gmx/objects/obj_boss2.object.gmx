        <arguments>
          <argument>
            <kind>1</kind>
            <string>///State swapping
if (hp &lt;= 0)
{
    combatState = "stand";
}
else
{
switch(combatState)
{
    case ("stand"):
    {
        //Stop sizzle sound if playing
        if(audio_is_playing(snd_sizzle))
                audio_stop_sound(snd_sizzle);
    
        //Set attack reload timer
        dmgTaken = hpMax - hp;
        attackTimer = maxAttackTime - (dmgTaken * atkTimerScale);
    
        if (numAttacks &lt; numAttacksTillStun)
        {
            //Need to attack more before overheating    
        
            randomize()
            
            //Make next attack unique (no same move in a row)
            nextAttack = ds_list_find_value(attackList,floor(random(3)));
            while(nextAttack == lastAttack)
            {
                randomize()
                nextAttack = ds_list_find_value(attackList,floor(random(3)));
            }
            
            combatState = nextAttack
            lastAttack = nextAttack
            
            numAttacks++;
        }
        else
        {
            //Start overheat
            
            numAttacks = 0; //reset num attacks for next cycle
            
            //Set overheat timer
            dmgTaken = hpMax - hp;
            overheatTimer = overheatTime - (dmgTaken * overheatTimerScale);
            
            //Play sizzle
            if(!audio_is_playing(snd_sizzle))
                audio_play_sound(snd_sizzle,5,true);
            
            combatState = "overheat";
        }
        break;
    }
    
    case ("missiles"):
    {
        xCent = x+sprite_width/2;
        yCent = y+sprite_height/2;
        missile1 = instance_create(xCent, yCent - 1, obj_missile);
        missile2 = instance_create(xCent-1, yCent - 1, obj_missile);
        missile3 = instance_create(xCent+1, yCent - 1, obj_missile);
        if (missileNum &gt;= 4)
        {
            missile4 = instance_create(xCent-2, yCent - 1, obj_missile);
        }
        if (missileNum &gt;=5)
        {
            missile5 = instance_create(xCent+2, yCent - 1, obj_missile);
        }
        
        combatState = "reload";
        break;
    }
    
    case ("summon"):
    {
        summon1 = instance_create(11264,2688,obj_enemy_spawning);
        summon1.temp = true;
        summon2 = instance_create(10240,2688,obj_enemy_spawning);
        summon2.temp = true;
        if (missileNum &gt;= 4.5)
        {
            summon3 = instance_create(x+32,yCent,obj_enemy_spawning);
            summon3.temp = true;
        }
        
        //Now that enemies have telegraph time, give player time to kill them
        attackTimer += 32;
        
        combatState = "reload";
        break;
    }
    
    case ("laser"):
    {
        laser1 = instance_create(x,y,obj_laserParent);
        laser2 = instance_create(x,y,obj_laserParent);
        laser3 = instance_create(x,y,obj_laserParent);
        laser3.targX = view_xview + view_wview/2;
        laser3.targY = view_yview + view_hview/2;
        laser1.image_angle = laser2.image_angle + 90;
        if (laser3.image_angle == 90)
        {
            laser3.image_angle = 270;
        }
        
        combatState = "reload";
        break;
    }
    
    case ("reload"):
    {
        if (attackTimer &gt; 0)
        {
            attackTimer --;
        }
        else
        {
            combatState = "stand";
        }
        break;
    }
    
    case ("overheat"):
    {
        if (overheatTimer &gt; 0)
        {
            overheatTimer --;
        }
        else
        {
            combatState = "stand";
        }
        break;
    }
}
}
</string>
