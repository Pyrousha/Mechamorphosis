            <string>audio_stop_sound(global.introSong);
audio_stop_sound(snd_song2_intro);
audio_stop_sound(snd_song2_loop);

textList = ds_list_create()
ds_list_add(textList,"Story/Writing:#Maggie Hendon# #Animations/Artwork:#Tosia Xiong# #Level Design:#Travis Bird + Maggie Hendon# #Programming/Combat Design:#Travis Bird##");
i=0;

text = (ds_list_find_value(textList,i))
